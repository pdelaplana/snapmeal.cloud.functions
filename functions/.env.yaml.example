# Environment variables template for Firebase Functions v2
# This file serves as documentation - the actual .env.yaml is generated during deployment

# Database configuration
DATABASE_ID: development  # Can be: development, staging, production, or any other database ID

# Application environment
ENVIRONMENT: production
DEBUG_MODE: false

# Note: Secrets like SENTRY_DSN, MAILGUN_API_KEY, and MAILGUN_DOMAIN
# should be managed through Firebase Secret Manager or GitHub Secrets
# They are not included in .env.yaml for security reasons
